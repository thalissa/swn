<div class="tab notes" data-group="primary" data-tab="notes">
  {{!-- Notes To Remember Textfield --}}
  <div class="flexcol">
    <div class="inputName">
      {{ localize "SWN.Notes.ToRemember" }}
    </div>
  </div>
  {{editor content=notes.remember target="data.notes.remember" button=true owner=owner editable=editable rollData=rollData}}
  <hr/>
  {{!-- Allies Textfield --}}
  <div class="flexcol inputTextfield">
    <div class="inputName">
      {{ localize "SWN.Notes.Allies" }}
    </div>
  </div>
  {{editor content=notes.allies target="data.notes.allies" button=true owner=owner editable=editable rollData=rollData}}
  <hr/>
  {{!-- Enemies Textfield --}}
  <div class="flexcol inputTextfield">
    <div class="inputName">
      {{ localize "SWN.Notes.Enemies" }}
    </div>
  </div>
  {{editor content=notes.enemies target="data.notes.enemies" button=true owner=owner editable=editable rollData=rollData}}
  <hr/>
  {{!-- Other Textfield --}}
  <div class="flexcol inputTextfield">
    <div class="inputName">
      {{ localize "SWN.Notes.Other" }}
    </div>
  </div>
  {{editor content=notes.other target="data.notes.other" button=true owner=owner editable=editable rollData=rollData}}
</div>
